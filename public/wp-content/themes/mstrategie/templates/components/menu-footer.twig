{% if menu %}
<div class="menu-footer">
	<ul class="menu-footer_items">
		{% for item in items %}
		<li class="menu-footer-item">
			<a 
				{# target="{{ item.target }}" #}
				href="{{ item.link }}"
			>
				{{ item.title }}
			</a>
			{#{% include "components/menu.twig" with {'items': item.children} %}#}
		</li>
		{% endfor %}
	</ul>
</div>
{% endif %}